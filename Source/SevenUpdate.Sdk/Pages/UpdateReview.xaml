<Page x:Class="SevenUpdate.Sdk.Pages.UpdateReview" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:Controls="clr-namespace:Microsoft.Windows.Controls;assembly=Windows.Shell" mc:Ignorable="d" MinWidth="850" MinHeight="512" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Title="SevenUpdate" KeepAlive="True" d:DesignHeight="467" d:DesignWidth="800">
	<!-- Grid-->
	<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Name="grid">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="826*" />
			<ColumnDefinition Width="24" />
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="32" />
			<RowDefinition Height="420*" />
			<RowDefinition Height="44" />
