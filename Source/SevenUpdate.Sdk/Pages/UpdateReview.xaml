				</TextBlock.Effect>
			</TextBlock>
		</StackPanel>
		<Rectangle Grid.Row="1" HorizontalAlignment="Stretch" Name="rectangle" Stroke="Black" VerticalAlignment="Stretch" Grid.ColumnSpan="2" />
	</Grid>
</Page>