  </Style>

  <Style TargetType="controls:InfoTextBox">
    <Setter Property="FontSize" Value="14" />
    <Setter Property="NoteStyle" Value="{StaticResource shadowedLabelStyle}" />
    <Setter Property="Validation.ErrorTemplate">
      <Setter.Value>
        <ControlTemplate>
          <!--<Grid>
                        <AdornedElementPlaceholder x:Name="PART_ContentHost" />
                        <Rectangle Fill="{StaticResource ErrorIcon}" Width="18" HorizontalAlignment="Right" Margin="0,0,4,0" />
                    </Grid>-->
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Style.Triggers>
      <Trigger Property="Validation.HasError" Value="true">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="controls:InfoTextBox">
              <Grid>
                <Border BorderBrush="DarkRed" x:Name="Bd" BorderThickness="1" CornerRadius="1" Padding="1"
                    SnapsToDevicePixels="True">
                  <ScrollViewer x:Name="PART_ContentHost" />
                </Border>
                <Rectangle Fill="{StaticResource ErrorIcon}" Width="18" HorizontalAlignment="Right" Margin="0,0,4,0" />
              </Grid>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
        <Setter Property="ToolTip"
            Value="{Binding RelativeSource={x:Static RelativeSource.Self}, Path=(Validation.Errors)[0].ErrorContent}" />
      </Trigger>
      <Trigger Property="HasError" Value="true">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="controls:InfoTextBox">
              <Grid>
                <Border BorderBrush="DarkRed" x:Name="Bd" BorderThickness="1" CornerRadius="1" Padding="1"
                    SnapsToDevicePixels="True">
                  <ScrollViewer x:Name="PART_ContentHost" />
                </Border>
                <Rectangle Fill="{StaticResource ErrorIcon}" Width="18" HorizontalAlignment="Right" Margin="0,0,4,0" />
              </Grid>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Trigger>
      <Trigger Property="HasWarning" Value="true">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="controls:InfoTextBox">
              <Grid>
                <Border BorderBrush="DarkOrange" x:Name="Bd" BorderThickness="1" CornerRadius="1" Padding="1"
                    SnapsToDevicePixels="True">
                  <ScrollViewer x:Name="PART_ContentHost" />
                </Border>
                <Rectangle Fill="{StaticResource WarningIcon}" Width="18" HorizontalAlignment="Right" Margin="0,0,4,0" />
              </Grid>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Trigger>
    </Style.Triggers>
  </Style>
