<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:controls="clr-namespace:System.Windows.Controls">
    <BooleanToVisibilityConverter x:Key="VisibilityOfBool" />
    
    <!--CommandLink Style-->
    <Style TargetType="controls:CommandLink">
        <Setter Property="MinWidth" Value="325" />
        <Setter Property="MinHeight" Value="63" />
        <Setter Property="Margin" Value="0,0,0,12" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:CommandLink">
                    <Border Name="Border" CornerRadius="2" BorderBrush="Transparent" BorderThickness="1" SnapsToDevicePixels="True">
                        <Border Name="InnerBorder" CornerRadius="2" BorderBrush="Transparent" BorderThickness="1" SnapsToDevicePixels="True">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0,1">
                                    <GradientStop Offset="0" />
                                    <GradientStop Offset="0.05" />
                                    <GradientStop Offset="0.05" />
                                    <GradientStop Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <StackPanel Margin="7,7,7,7" Orientation="Horizontal">
                                <Image Name="Icon" Source="/System.Windows;component/Images/GreenArrow.png" RenderOptions.BitmapScalingMode="NearestNeighbor" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="None" Margin="0,4,5,0" Width="18" Height="20" />
                                <StackPanel Margin="6,0,0,10">
                                    <TextBlock Name="InstructionText" FontFamily="Segoe" FontSize="16" Foreground="#282671" Text="{TemplateBinding Content}">
                                        <TextBlock.Style>
                                            <Style>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Source={x:Static controls:CommandLink.RunningOnXP}}" Value="False">
                                                        <Setter Property="TextBlock.Effect">
                                                            <Setter.Value>
                                                                <DropShadowEffect Color="White" BlurRadius="10" RenderingBias="Performance" ShadowDepth="0" Direction="0" />
                                                            </Setter.Value>
                                                        </Setter>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                    <TextBlock Name="Note" FontFamily="Segoe" FontSize="12" Foreground="#282671" Text="{Binding Path=Note, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Button}}}">
                                        <TextBlock.Style>
                                            <Style>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Source={x:Static controls:CommandLink.RunningOnXP}}" Value="5">
                                                        <Setter Property="TextBlock.Effect">
                                                            <Setter.Value>
                                                                <DropShadowEffect Color="White" BlurRadius="10" RenderingBias="Performance" ShadowDepth="0" Direction="0" />
                                                            </Setter.Value>
                                                        </Setter>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </StackPanel>
                            </StackPanel>
                        </Border>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="UseShield" Value="True">
                            <Setter TargetName="Icon" Property="Image.Source" Value="/System.Windows;component/Images/Shield.png" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsMouseOver" Value="True" />
                                <Condition Property="UseShield" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Icon" Property="Image.Source" Value="/System.Windows;component/Images/GreenArrowHover.png" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsPressed" Value="True" />
                                <Condition Property="UseShield" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Icon" Property="Image.Source" Value="/System.Windows;component/Images/GreenArrow.png" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsDefaulted" Value="True" />
                                <Condition Property="IsPressed" Value="False" />
                                <Condition Property="IsMouseOver" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Border" Property="BorderBrush" Value="#66e0ff" />
                            <Setter TargetName="InstructionText" Property="Foreground" Value="#282671" />
                            <Setter TargetName="Note" Property="Foreground" Value="#282671" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsKeyboardFocused" Value="True" />
                                <Condition Property="IsPressed" Value="False" />
                                <Condition Property="IsMouseOver" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Border" Property="BorderBrush" Value="#66e0ff" />
                            <Setter TargetName="InstructionText" Property="Foreground" Value="#282671" />
                            <Setter TargetName="Note" Property="Foreground" Value="#282671" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsDefaulted" Value="True" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" To="#5380f6" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#5380f6" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#66e0ff" To="#c6c6c6" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="White" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#ffffff" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#f4f4f4" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.EnterActions>
                            <MultiTrigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" From="#5380f6" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#5380f6" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#c6c6c6" To="#66e0ff" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="White" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#ffffff" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#f4f4f4" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.ExitActions>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsKeyboardFocused" Value="True" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" To="#5380f6" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#5380f6" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#66e0ff" To="#c6c6c6" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="White" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#ffffff" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#f4f4f4" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.EnterActions>
                            <MultiTrigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" From="#5380f6" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#5380f6" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#c6c6c6" To="#66e0ff" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="White" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#ffffff" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#f4f4f4" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.ExitActions>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsDefaulted" Value="False" />
                                <Condition Property="IsKeyboardFocused" Value="False" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" To="#5380f6" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#5380f6" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#c6c6c6" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="White" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#ffffff" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#f4f4f4" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.EnterActions>
                            <MultiTrigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" From="#5380f6" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#5380f6" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#c6c6c6" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="White" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#ffffff" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#f4f4f4" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </MultiTrigger.ExitActions>
                        </MultiTrigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" To="#294073" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#294073" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#a8a8a8" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#e8e9e9" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#dadada" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[0].Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#f0f1f1" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[1].Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#f0f1f1" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" To="#f0f1f1" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation Duration="0:0:.5" From="#294073" Storyboard.TargetName="InstructionText" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#294073" Storyboard.TargetName="Note" Storyboard.TargetProperty="Foreground.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#a8a8a8" Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#e8e9e9" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="BorderBrush.Color" />
                                        <ColorAnimation Duration="0:0:.5" From="Gainsboro" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[0].Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#f0f1f1" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[1].Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#f0f1f1" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[2].Color" />
                                        <ColorAnimation Duration="0:0:.5" From="#f0f1f1" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Background.GradientStops[3].Color" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!--Back Button Style-->
    <Style TargetType="controls:BackButton">
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:BackButton">
                    <Canvas MinWidth="28" MinHeight="28">
                        <Image Name="Normal" Source="/System.Windows;component/Images/BackArrow.png" RenderOptions.BitmapScalingMode="HighQuality" Stretch="None" Opacity="1" />
                        <Image Name="Hover" Source="/System.Windows;component/Images/BackArrowHover.png" RenderOptions.BitmapScalingMode="HighQuality" Stretch="None" Opacity="0" />
                        <Image Name="Pressed" Source="/System.Windows;component/Images/BackArrowPressed.png" RenderOptions.BitmapScalingMode="HighQuality" Stretch="None" Opacity="0" />
                        <Image Name="Selected" Source="/System.Windows;component/Images/BackArrowPressed.png" RenderOptions.BitmapScalingMode="HighQuality" Stretch="None" Opacity="0" />
                        <Image Name="Disabled" Source="/System.Windows;component/Images/BackArrowDisabled.png" RenderOptions.BitmapScalingMode="HighQuality" Stretch="None" Opacity="0" />
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <DataTrigger Value="False" Binding="{Binding Path=IsEnabled, RelativeSource={RelativeSource Self}}">
                            <DataTrigger.Setters>
                                <Setter TargetName="Disabled" Property="Image.Opacity" Value=".7" />
                                <Setter TargetName="Normal" Property="Image.Opacity" Value="0" />
                            </DataTrigger.Setters>
                        </DataTrigger>
                        <!--<DataTrigger Value="True" Binding="{Binding Path=IsChecked, RelativeSource={RelativeSource Self}}">
                            <DataTrigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="Selected" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation AccelerationRatio="0.2" DecelerationRatio="0.3" From="0.3" To="0.7" Duration="0:0:2" AutoReverse="True" RepeatBehavior="Forever" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </DataTrigger.EnterActions>
                            <DataTrigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="Selected" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="0" Duration="0:0:1" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </DataTrigger.ExitActions>
                        </DataTrigger>-->
                        <EventTrigger RoutedEvent="Button.MouseEnter">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="Hover" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="1" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="Button.MouseLeave">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="Hover" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="0" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="Button.PreviewMouseLeftButtonDown">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="Pressed" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="1" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="Button.PreviewMouseLeftButtonUp">
                            <EventTrigger.Actions>
                                <BeginStoryboard>
                                    <Storyboard Storyboard.TargetName="Pressed" Storyboard.TargetProperty="Opacity">
                                        <DoubleAnimation To="0" Duration="0:0:0.5" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger.Actions>
                        </EventTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--Progress Indicator-->
    <Style x:Key="EllipseStyle" TargetType="Ellipse">
        <Style.Setters>
            <Setter Property="Width" Value="20" />
            <Setter Property="Height" Value="20" />
            <Setter Property="Fill">
                <Setter.Value>
                    <RadialGradientBrush>
                        <GradientStop Color="#CA2C8DDE" Offset="0.634" />
                        <GradientStop Color="#39FFFFFF" Offset="1" />
                        <GradientStop Color="#CA2C64DE" Offset="0.33" />
                        <GradientStop Color="#B56A8FDE" Offset="0.062" />
                    </RadialGradientBrush>
                </Setter.Value>
            </Setter>
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <ScaleTransform ScaleX="0" ScaleY="0" CenterX="12.5" CenterY="12.5" />
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style TargetType="controls:ProgressIndicator">
        <Style.Setters>
            <Setter Property="Height" Value="20" />
            <Setter Property="Width" Value="20" />
            <Setter Property="ElementStoryboard">
                <Setter.Value>
                    <Storyboard>
                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)">
                            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
                            <SplineDoubleKeyFrame KeyTime="00:00:00.5" Value="1" />
                            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
                            <SplineDoubleKeyFrame KeyTime="00:00:01.5" Value="0" />
                        </DoubleAnimationUsingKeyFrames>
                        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleY)">
                            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
                            <SplineDoubleKeyFrame KeyTime="00:00:00.5" Value="1" />
                            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
                            <SplineDoubleKeyFrame KeyTime="00:00:01.5" Value="0" />
                        </DoubleAnimationUsingKeyFrames>
                    </Storyboard>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="controls:ProgressIndicator">
                        <Grid Visibility="{TemplateBinding IsRunning, Converter={StaticResource VisibilityOfBool}}">
                            <Canvas x:Name="PART_Canvas" SnapsToDevicePixels="True" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                                <Ellipse Style="{StaticResource EllipseStyle}" />
                            </Canvas>
                            <TextBlock Name="LoadingTextBlock" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IndeterminateText}" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" TextAlignment="Center" />
                            <TextBlock Name="ProgressTextBlock" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Value}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsIndeterminate" Value="True">
                                <Setter Property="Visibility" TargetName="ProgressTextBlock" Value="Hidden" />
                            </Trigger>
                            <Trigger Property="IsIndeterminate" Value="False">
                                <Setter Property="Visibility" TargetName="LoadingTextBlock" Value="Hidden" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style x:Key="shadowedLabelStyle">
        <Setter Property="TextBlock.Foreground" Value="{x:Static SystemColors.ControlDarkBrush}" />
        <Setter Property="FrameworkElement.Opacity" Value="0.8" />
        <Setter Property="TextBlock.FontSize" Value="12" />
        <Setter Property="TextBlock.FontStyle" Value="Italic" />
        <Setter Property="TextBlock.Margin" Value="8,4,4,4" />
    </Style>

    <!--Validation TextBox-->
    <Style TargetType="controls:InfoTextBox">
        <Setter Property="FontSize" Value="14" />
        <Setter Property="NoteStyle" Value="{StaticResource shadowedLabelStyle}" />
        <Setter Property="Validation.ErrorTemplate">
            <Setter.Value>
                <ControlTemplate>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                        <AdornedElementPlaceholder />
                        <Image Source="/System.Windows;component/Images/Caution.png" Margin="4,0" SnapsToDevicePixels="True" RenderOptions.BitmapScalingMode="NearestNeighbor" />
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="Validation.HasError" Value="true">
                <Setter Property="BorderBrush" Value="DarkOrange" />
                <Setter Property="ToolTip" Value="{Binding RelativeSource={x:Static RelativeSource.Self}, Path=(Validation.Errors)[0].ErrorContent}" />
            </Trigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>